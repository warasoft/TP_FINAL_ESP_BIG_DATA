LOAD

./bin/ycsb load mongodb -s -P workloads/workloada -p recordcount=500000 -threads 4 -p mongodb.url=mongodb://root:j@localhost:27017/admin
###./bin/ycsb load mongodb -s -P workloads/workloada -p mongodb.url=mongodb://root:j@localhost:27017/admin
###./bin/ycsb load mongodb -s -P workloads/workloada -p mongodb.url=mongodb://localhost:27017/ycsb?w=0


RUN
./bin/ycsb run mongodb -s -P workloads/workloada -p mongodb.url=mongodb://root:j@localhost:27017/admin -p operationcount=500000 -threads 2
###./bin/ycsb run mongodb -s -P workloads/workloada -p mongodb.url=mongodb://root:j@localhost:27017/admin





